<Window x:Class="szamologep.wpfapp.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp1"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="szam1" HorizontalAlignment="Left" Margin="90,150,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="szam2" HorizontalAlignment="Left" Margin="244,150,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="plus" Content="+" HorizontalAlignment="Left" Margin="163,247,0,0" VerticalAlignment="Top" Click="plus_Click"/>
        <Button x:Name="minus" Content="-" HorizontalAlignment="Left" Margin="225,247,0,0" VerticalAlignment="Top" Click="minus_Click"/>
        <Button x:Name="multiple" Content="*" HorizontalAlignment="Left" Margin="163,286,0,0" VerticalAlignment="Top" Click="multiple_Click"/>
        <Button x:Name="divide" Content="/" HorizontalAlignment="Left" Margin="225,286,0,0" VerticalAlignment="Top" Click="divide_Click"/>
        <TextBox x:Name="kimenet" HorizontalAlignment="Left" Margin="163,80,0,0" TextWrapping="Wrap" Text="eredmeny" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="kilep" Content="exit" HorizontalAlignment="Left" Margin="585,286,0,0" VerticalAlignment="Top" Click="kilep_Click"/>

    </Grid>
</Window>
